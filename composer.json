{
    "name": "hyperf/biz-skeleton",
    "type": "project",
    "keywords": [
        "php",
        "swoole",
        "framework",
        "hyperf",
        "microservice",
        "middleware"
    ],
    "description": "A coroutine framework that focuses on hyperspeed and flexible, specifically use for build microservices and middlewares.",
    "license": "Apache-2.0",
    "require": {
        "php": ">=7.2",
        "ext-json": "*",
        "ext-openssl": "*",
        "ext-pdo": "*",
        "ext-pdo_mysql": "*",
        "ext-redis": "*",
        "ext-swoole": ">=4.2",
        "hyperf/amqp": "*",
        "hyperf/async-queue": "*",
        "hyperf/cache": "*",
        "hyperf/circuit-breaker": "*",
        "hyperf/command": "*",
        "hyperf/config": "*",
        "hyperf/constants": "*",
        "hyperf/contract": "*",
        "hyperf/database": "*",
        "hyperf/db-connection": "*",
        "hyperf/devtool": "*",
        "hyperf/di": "*",
        "hyperf/dispatcher": "*",
        "hyperf/elasticsearch": "*",
        "hyperf/event": "*",
        "hyperf/exception-handler": "*",
        "hyperf/framework": "*",
        "hyperf/guzzle": "*",
        "hyperf/http-server": "*",
        "hyperf/logger": "*",
        "hyperf/memory": "*",
        "hyperf/model-cache": "*",
        "hyperf/paginator": "*",
        "hyperf/pool": "*",
        "hyperf/process": "*",
        "hyperf/redis": "*",
        "hyperf/utils": "*",
        "hyperf/json-rpc": "*",
        "hyperf/rpc-server": "*",
        "hyperf/rpc-client": "*",
        "symfony/property-access": "^4.3",
        "symfony/serializer": "^4.3"
    },
    "require-dev": {
        "swoft/swoole-ide-helper": "^4.2",
        "phpmd/phpmd": "^2.6",
        "friendsofphp/php-cs-fixer": "^2.14",
        "mockery/mockery": "^1.0",
        "doctrine/common": "^2.9",
        "phpstan/phpstan": "^0.11.2",
        "hyperf/testing": "*"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/Kernel/Functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "HyperfTest\\": "./test/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "extra": [],
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "test": "co-phpunit -c phpunit.xml --colors=always",
        "cs-fix": "php-cs-fixer fix $1",
        "analyze": "phpstan analyse --memory-limit 300M -l 0 -c phpstan.neon ./app ./config",
        "start": "php ./bin/hyperf.php start"
    },
    "repositories": {
        "hyperf": {
            "type": "path",
            "url": "../hyperf/src/*"
        },
        "packagist": {
            "type": "composer",
            "url": "https://mirrors.aliyun.com/composer"
        }
    }
}
